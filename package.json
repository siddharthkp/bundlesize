{
  "name": "bundlesize",
  "version": "0.13.2",
  "description": "Keep your library size in check",
  "main": "index.js",
  "bin": {
    "bundlesize": "index.js",
    "bundlesize-init": "src/init-status.js"
  },
  "scripts": {
    "test": "node index"
  },
  "keywords": [
    "library",
    "size",
    "check",
    "build",
    "maxSize"
  ],
  "files": [
    "index.js",
    "src"
  ],
  "author": "siddharthkp",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "bytes": "^2.5.0",
    "ci-env": "^1.4.0",
    "commander": "^2.11.0",
    "github-build": "^1.2.0",
    "glob": "^7.1.2",
    "gzip-size": "^3.0.0",
    "prettycli": "^1.3.0",
    "read-pkg-up": "^2.0.0"
  },
  "bundlesize": [
    {
      "path": "./index.js",
      "maxSize": "600B"
    },
    {
      "path": "./src/files.js",
      "maxSize": "600B"
    }
  ]
}
